{
  "name": "webtools",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port ${PORT:=3000}",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start --port ${PORT:=3000}",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "rimraf node_modules && rimraf .next",
    "lint": "TIMING=1 next lint"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.11",
    "@fontsource/jetbrains-mono": "^4.5.9",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@tabler/icons": "^1.68.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "fast-geoip": "^1.1.79",
    "framer-motion": "^6.3.11",
    "next": "12.1.6",
    "next-plausible": "^3.2.0",
    "next-themes": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.3.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@types/node": "^17.0.42",
    "@types/react": "^17.0.43",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "eslint": "~8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "flag-icons": "^6.4.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  },
  "config": {
    "container": "riipandi/next-tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 110,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
